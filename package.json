{
  "name": "cryptoleet",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && mkdir -p ./dist/assets/img && cp -R ./src/assets/img ./dist/assets/",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "styleguide": "concurrently \"vue-docgen src/**/[a-zA-Z]*.{vue,js,jsx,tsx} docs --watch\" \"vitepress dev docs\"",
    "styleguide:build": "vue-docgen ./src/**/[a-zA-Z]*.{vue} docs && vitepress build docs",
    "styleguide2": "vue-styleguidist server"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.10",
    "@heroicons/vue": "^2.0.16",
    "@vueuse/core": "^9.12.0",
    "axios": "^1.3.3",
    "chart.js": "^3.9.1",
    "prismjs": "^1.29.0",
    "vue": "^3.2.47",
    "vue-chart-3": "^3.1.8",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.1.6",
    "vue-toast-notification": "^3.1.2",
    "vue3-lazyload": "^0.3.6"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.3.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^8.2.2",
    "es-module-lexer": "^1.4.1",
    "eslint": "^8.53.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "globby": "^14.0.0",
    "postcss": "^8.4.21",
    "sass": "^1.58.2",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitepress": "^1.0.0-rc.27",
    "vue-component-meta": "^1.8.22",
    "vue-docgen-cli": "^4.67.0",
    "vue-live": "^2.5.4",
    "vue-tsc": "^1.1.3"
  }
}
